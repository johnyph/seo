<div class="panel panel-default login-form space-20">
  <div class="panel-heading">
    <h3 class="panel-title">Link-info</h3>
  </div>
  <div class="panel-body">
    <table class="table table-striped">
      <tbody>
        
        <tr>
          <td>link</td>
          <td><%= @link.link %></td>
        </tr>

        <tr>
          <td>Keyword: </td>
          <td><%= @link.result.keyword.name %></td>
        </tr>

        <tr>
          <td>Occurrences</td>
          <td><%= @occurrences %></td>
        </tr>
 
      </tbody>

    </table>
    <div class="space-20"></div>
    <%= link_to "Queries", queries_path, class:"btn btn-primary" %>
  </div>

</div>